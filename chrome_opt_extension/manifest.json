{
  "name": "Chrome Optimization",
  "version": "0.1",
  "description": "Optimize Chrome testing.",
  "permissions": ["browsingData","tabs","webRequest","<all_urls>"],
  "background": {
    "scripts": ["jquery-2.2.1.min.js","background.js"]
  },
  "content_scripts":[
      {
        "js":["actions.js","jquery-2.2.1.min.js"],
        "matches":["<all_urls>"]
      }
    ],
  "web_accessible_resources": [
    "log.json"
    ],
  "manifest_version": 2
}
