{
  "name": "Clear-Cache",
  "version": "0.1",
  "description": "Clear browser's cache.",
  "permissions": ["browsingData", "tabs"],
  "commands": {
    "clear-cache": {
      "suggested_key": {
        "default": "Alt+Y"
      },
      "description": "Clear browser cache."
    }
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2
}
